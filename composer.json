{
    "name": "mobileapps.vectorcoder/laravel-installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Amir Saleem",
            "homepage": "https://gitlab.com/mobileapps.vectorcoder",
            "role": "Manager",
            "email": "mobileapps.vectorcoder@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "mobileapps.vectorcoder/laravel-installer\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                {
                    "type": "vcs",
                    "url": "https://gitlab.com/mobileapps.vectorcoder/laravel-installer/"
                }
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability" : "dev"

}
